<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="zgames.zgames.Controller">
   <children>
      <VBox alignment="CENTER" prefHeight="600.0" prefWidth="800.0">
         <children>
            <VBox prefHeight="400.0" prefWidth="800.0">
               <children>
                  <VBox fx:id="videoList" prefHeight="350.0" prefWidth="800.0" />
                  <HBox alignment="CENTER" prefHeight="50.0" prefWidth="800.0">
                     <children>
                        <Button fx:id="btnBackwardVideoPage" mnemonicParsing="false" onAction="#btnBackwardVideoPage" text="&lt;" />
                        <Button fx:id="btnForwardVideoPage" mnemonicParsing="false" onAction="#btnForwardVideoPage" text="&gt;" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </VBox>
      <Button layoutY="1.0" mnemonicParsing="false" onAction="#innerListPage" text="Назад" />
   </children>
</AnchorPane>
