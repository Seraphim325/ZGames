<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane minWidth="-Infinity" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="zgames.zgames.Controller">
   <children>
      <VBox alignment="CENTER" prefHeight="600.0" prefWidth="800.0">
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <HBox fx:id="list" alignment="CENTER_LEFT" minWidth="-Infinity" prefHeight="200.0" prefWidth="800.0" />
                  <HBox alignment="CENTER" prefHeight="34.0" prefWidth="800.0">
                     <VBox.margin>
                        <Insets left="5.0" right="5.0" />
                     </VBox.margin>
                     <children>
                        <HBox fx:id="btns" alignment="CENTER" nodeOrientation="LEFT_TO_RIGHT" prefHeight="100.0" prefWidth="200.0">
                           <children>
                              <Button fx:id="btnBackward" mnemonicParsing="false" onAction="#btnBackward" text="&lt;" />
                              <Button fx:id="btnForward" mnemonicParsing="false" onAction="#btnForward" text="&gt;" />
                           </children>
                        </HBox>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </VBox>
    <Button mnemonicParsing="false" onAction="#startPage" text="Назад" />
   </children>
</AnchorPane>
